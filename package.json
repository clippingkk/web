{
  "name": "kindle-viewer-website",
  "version": "1.0.0",
  "description": "kindle-viewer-website",
  "main": "index.js",
  "scripts": {
    "dev:dll": "cross-env NODE_ENV=development webpack --config scripts/webpack.dll.config.js",
    "dev": "cross-env NODE_ENV=development webpack-serve --config scripts/webpack.dev.config.js --port 8080 --open",
    "prod:dll": "cross-env NODE_ENV=production webpack -p --config scripts/webpack.dll.config.js",
    "prod": "cross-env NODE_ENV=production webpack -p --config scripts/webpack.prod.config.js",
    "mock": "json-server mock/db.js --routes mock/routes.json --port 9000",
    "zip": "bestzip dist.zip dist/*",
    "build": "npm run prod:dll && npm run prod && npm run zip",
    "test": "cross-env NODE_ENV=test ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git.annatarhe.com/annatarhe/kindle.annatarhe.com"
  },
  "keywords": [],
  "author": "AnnatarHe<iamhele1994@gamil.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://git.annatarhe.com/annatarhe/kindle.annatarhe.com/issues"
  },
  "homepage": "https://git.annatarhe.com/annatarhe/kindle.annatarhe.com#readme",
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup-test-env.js"
    ]
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.3",
    "autoprefixer": "^7.1.2",
    "ava": "^0.24.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bestzip": "^2.1.1",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.11",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "jsdom": "^11.5.1",
    "json-server": "^0.12.0",
    "koa-connect": "^2.0.1",
    "mini-css-extract-plugin": "^0.4.1",
    "postcss-loader": "^2.1.5",
    "postcss-modules-values": "^1.3.0",
    "poststylus": "^1.0.0",

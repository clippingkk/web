    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^1.0.4"
  },
  "dependencies": {
    "react": "16",
    "react-dom": "16",
    "react-hot-loader": "^4.3.3",
    "react-markdown": "^4.0.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.2.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "whatwg-fetch": "^2.0.3"
  }
